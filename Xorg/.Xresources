! special
*.foreground:   #ababab
*.background:   #000000
*.cursorColor:  #5e81ac

! black
*.color0:       #000000
*.color8:       #343434

! red
*.color1:       #e92f2f
*.color9:       #e92f2f

! green
*.color2:       #0ed839
*.color10:      #0ed839

! yellow
*.color3:       #dddd13
*.color11:      #dddd13

! blue
*.color4:       #3b48e3
*.color12:      #3b48e3

! magenta
*.color5:       #f996e2
*.color13:      #f996e2

! cyan
*.color6:       #23edda
*.color14:      #23edda

! white
*.color7:       #ababab
*.color15:      #f9f9f9

! Transparency
!URxvt.transparent: true
!URxvt.shading: 100


URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.font:     xft:MesloLGS NF:size=8
URxvt.boldFont: xft:MesloLGS NF:size=8
URxvt.letterSpace: 1
URxvt.perl-ext-common: default,matcher,resize-font,selection-to-clipboard
URxvt.font-size.step: 1
URxvt.matcher.button: 1
URxvt.url-launcher: /usr/bin/brave
URxvt.cursorBlink: true
URxvt.intensityStyles: true
URxvt.tabbed.tabbar-fg: 3
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 1
URxvt.tabbed.tab-bg: 0

